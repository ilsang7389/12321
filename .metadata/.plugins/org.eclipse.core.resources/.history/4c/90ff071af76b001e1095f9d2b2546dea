package ex6;

public class Ex1_AutoWirde {
	private String msg;

	
	public void setMsg(String msg) {
		this.msg = msg;
	}
	public String getMessage() {
		StringBuffer sb = new StringBuffer();
		sb.append("<p style ='color:blue");
		sb.append("msg");
		sb.append("</P>");
		return sb.toString();
	}
	

}
