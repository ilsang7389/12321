package ex3;

public class DaoImple implements DaoInter{

	public DaoImple() {
		System.out.println("DaoImple 생성자 호출!");
	}
	@Override
	public void first() {
		for(int i=0 ; 1 <10; i++) {
			try {
				Thread.sleep(1000);
				System.out.println();
			} catch (InterruptedException e) {
				e.printStackTrace();
			}
		}
	}

	@Override
	public String second() {
		return null;
	}

	@Override
	public void third() {
		
	}

	@Override
	public String firstStatementTest(int code) {
		return null;
	}

}
